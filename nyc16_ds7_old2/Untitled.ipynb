    "def is_balanced(par_string):\n",
    "    count = 0\n",
    "    for i in par_string:\n",
    "        if i == '(':\n",
    "            count +=1\n",
    "        if i == ')':\n",
    "            count +=-1\n",
    "        if count < 0:\n",
    "            return False\n",
    "    return count == 0 \n",
    "\n",
    "str1 = '(()()))(()'\n",
    "\n",
    "print is_balanced(str1)"
